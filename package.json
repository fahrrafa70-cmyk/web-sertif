{
  "name": "sertificate-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "build": "next build",
    "build:turbo": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "clean": "node scripts/clear-cache.js",
    "fix:build": "node scripts/fix-build-errors.js",
    "fix": "node scripts/fix-build-errors.js",
    "clean:win": "powershell -Command \"Get-Process node -ErrorAction SilentlyContinue | Stop-Process -Force -ErrorAction SilentlyContinue; Start-Sleep -Seconds 1; if (Test-Path .next) { Remove-Item -Recurse -Force .next; Write-Host 'Cache cleared' } else { Write-Host 'No cache to clear' }; if (Test-Path .turbo) { Remove-Item -Recurse -Force .turbo; Write-Host 'Turbo cache cleared' }\"",
    "clean:unix": "rm -rf .next .turbo && echo Cache cleared",
    "dev:clean": "npm run clean:win && npm run dev",
    "migrate:storage": "node scripts/migrate-to-storage.js",
    "migrate:storage:dry": "node scripts/migrate-to-storage.js --dry-run",
    "migrate:storage:all": "node scripts/migrate-to-storage.js --all",
    "check:storage": "node scripts/check-storage-status.js",
    "analyze": "set ANALYZE=true && npm run build",
    "analyze:server": "npm run analyze && npx serve .next/static -p 3001",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "perf:baseline": "npm run build && npm run analyze:bundle",
    "perf:test": "npm run build && npm run analyze:bundle && echo Performance test completed",
    "pwa:validate": "node scripts/validate-pwa.js",
    "pwa:test": "npm run build && npm run pwa:validate",
    "thumbnails:regenerate": "node scripts/regenerate-thumbnails.js",
    "thumbnails:check": "echo 'Checking thumbnail optimization status...' && ls -la public/template/thumbnails/ 2>/dev/null || echo 'No thumbnails directory found'",
    "migrate:xid": "node scripts/migrate-uuid-to-xid.js migrate",
    "migrate:xid:verify": "node scripts/migrate-uuid-to-xid.js verify",
    "whitelist:add": "node scripts/add-admin-whitelist.js add",
    "whitelist:list": "node scripts/add-admin-whitelist.js list",
    "whitelist:remove": "node scripts/add-admin-whitelist.js remove",
    "sync:auth-whitelist": "node scripts/sync-auth-to-whitelist.js sync",
    "sync:auth-whitelist:dry": "node scripts/sync-auth-to-whitelist.js sync --dry-run",
    "sync:auth-whitelist:verify": "node scripts/sync-auth-to-whitelist.js verify"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/supabase-js": "^2.75.0",
    "@tabler/icons-react": "^3.35.0",
    "@tanstack/react-query": "^5.90.7",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-table": "^8.21.3",
    "@types/qrcode": "^1.5.6",
    "@types/react-window": "^1.8.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.24",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.545.0",
    "next": "15.5.5",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.9",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-window": "^2.2.3",
    "recharts": "^2.15.4",
    "sharp": "^0.33.5",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2",
    "xlsx": "^0.18.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^16.0.1",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.2",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.16",
    "eslint": "^9",
    "eslint-config-next": "15.5.5",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
